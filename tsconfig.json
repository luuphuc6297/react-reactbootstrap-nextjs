{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@pages/*": ["pages/*"],
            "@containers": ["containers"],
            "@containers/*": ["containers/*"],
            "@components": ["components"],
            "@components/*": ["components/*"],
            "@sections": ["sections"],
            "@sections/*": ["sections/*"],
            "@interfaces": ["interfaces"],
            "@hooks": ["hooks"],
            "@contexts": ["contexts"],
            "@apis/*": ["apis/*"],
            "@configs/*": ["configs/*"],
            "@utils/*": ["utils/*"],
            "@services/*": ["services/*"],
            "@lib/*": ["lib/*"],
            "@app/*": ["app/*"],
            "@models/*": ["models/*"],
            "@forms/*": ["forms/*"],
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
